{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "cli": "node --no-warnings --loader ts-node/esm src/cli/cli.ts",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^18.17.17",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "^0.17.4",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "got": "^13.0.0",
    "tslib": "^2.8.1"
  }
}
